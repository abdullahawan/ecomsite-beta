{{> header}}

<fieldset> 
    <form action="/order/search" method="POST">
        First Name: <br> 
        <input type="text" name="firstname" required> <br>
        Last Name: <br> 
        <input type="text" name="lastname" required> <br> 
        Email: <br>
        <input type="email" name="email" required>
        <br>
        <button type="submit">Submit Order</button>
    </form>
</fieldset>
